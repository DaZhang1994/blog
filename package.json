{
  "name": "blog",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node --max_old_space_size=12288 ./node_modules/@angular/cli/bin/ng build --prod --aot --build-optimizer --vendor-chunk --source-map=false ",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-scss": "cp -r projects/ng-uikit-pro-standard/src/lib/assets/ dist/ng-uikit-pro-standard",
    "copy-files": "cp projects/ng-uikit-pro-standard/src/lib/changelog projects/ng-uikit-pro-standard/src/lib/License.pdf dist/ng-uikit-pro-standard",
    "pack-archive": "cd dist/ng-uikit-pro-standard && npm pack . && cp ng-uikit-pro-standard-8.*.tgz ../..",
    "remove-archive": "rm dist/ng-uikit-pro-standard/ng-uikit-pro-standard-8.*.tgz && cd ../..",
    "compile": "ng build ng-uikit-pro-standard && npm run copy-scss && npm run copy-files && npm run pack-archive"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint"
    }
  },
  "private": true,
